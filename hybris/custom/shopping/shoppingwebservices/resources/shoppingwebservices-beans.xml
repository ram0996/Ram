<beans xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="beans.xsd">

	<enum class="com.shopping.web.services.populator.options.PaymentInfoOption">
		<value>BASIC</value>
		<value>BILLING_ADDRESS</value>
	</enum>

	<!--auth response  -->
	<bean class="com.shopping.web.services.auth.data.LogoutResponse">
		<property name="success" type="boolean"/>
	</bean>

	<!-- storesession data -->

	<bean class="com.shopping.web.services.storesession.data.LanguageDataList">
		<property name="languages"
		          type="java.util.Collection&lt;de.hybris.platform.commercefacades.storesession.data.LanguageData>"/>
	</bean>

	<bean class="com.shopping.web.services.storesession.data.CurrencyDataList">
		<property name="currencies"
		          type="java.util.Collection&lt;de.hybris.platform.commercefacades.storesession.data.CurrencyData>"/>
	</bean>

	<!-- order data -->

	<bean class="com.shopping.web.services.order.data.CartDataList">
		<property name="carts"
		          type="java.util.List&lt;de.hybris.platform.commercefacades.order.data.CartData>"/>
	</bean>

	<bean class="com.shopping.web.services.order.data.OrderEntryDataList">
		<property name="orderEntries"
		          type="java.util.List&lt;de.hybris.platform.commercefacades.order.data.OrderEntryData>"/>
	</bean>

	<bean class="com.shopping.web.services.order.data.CardTypeDataList">
		<property name="cardTypes"
		          type="java.util.List&lt;de.hybris.platform.commercefacades.order.data.CardTypeData>"/>
	</bean>

	<!-- product data -->

	<bean class="com.shopping.web.services.product.data.ProductDataList">
		<property name="products"
		          type="java.util.List&lt;de.hybris.platform.commercefacades.product.data.ProductData>"/>
		<property name="catalog" type="String"/>
		<property name="version" type="String"/>
		<property name="totalProductCount" type="int"/>
		<property name="totalPageCount" type="int"/>
		<property name="currentPage" type="int"/>
	</bean>

	<bean class="com.shopping.web.services.product.data.PromotionDataList">
		<property name="promotions"
		          type="java.util.List&lt;de.hybris.platform.commercefacades.product.data.PromotionData>"/>
	</bean>

	<bean class="com.shopping.web.services.product.data.PromotionResultDataList">
		<property name="promotions"
		          type="java.util.List&lt;de.hybris.platform.commercefacades.product.data.PromotionResultData>"/>
	</bean>

	<bean class="com.shopping.web.services.product.data.ReviewDataList">
		<property name="reviews" type="java.util.List&lt;de.hybris.platform.commercefacades.product.data.ReviewData>"/>
	</bean>

	<bean class="com.shopping.web.services.product.data.SuggestionDataList">
		<property name="suggestions"
		          type="java.util.List&lt;de.hybris.platform.commercefacades.product.data.SuggestionData>"/>
	</bean>

	<!-- user data -->

	<bean class="com.shopping.web.services.user.data.TitleDataList">
		<property name="titles" type="java.util.List&lt;de.hybris.platform.commercefacades.user.data.TitleData>"/>
	</bean>

	<bean class="com.shopping.web.services.user.data.CountryDataList">
		<property name="countries" type="java.util.List&lt;de.hybris.platform.commercefacades.user.data.CountryData>"/>
	</bean>

	<bean class="com.shopping.web.services.user.data.RegionDataList">
		<property name="regions"
				  type="java.util.List&lt;de.hybris.platform.commercefacades.user.data.RegionData>"/>
	</bean>

	<bean class="com.shopping.web.services.user.data.AddressDataList">
		<property name="addresses" type="java.util.List&lt;de.hybris.platform.commercefacades.user.data.AddressData>"/>
	</bean>

	<!-- voucher data -->
	<bean class="com.shopping.web.services.voucher.data.VoucherDataList">
		<property name="vouchers"
		          type="java.util.List&lt;de.hybris.platform.commercefacades.voucher.data.VoucherData>"/>
	</bean>

	<!-- validation data -->
	<bean class="com.shopping.web.services.validation.data.AddressValidationData">
		<property name="errors" type="de.hybris.platform.webservicescommons.dto.error.ErrorListWsDTO"/>
		<property name="decision" type="String"/>
		<property name="suggestedAddressesList"
		          type="com.shopping.web.services.user.data.AddressDataList"/>
	</bean>

	<bean class="com.shopping.web.services.validation.data.CartVoucherValidationData">
		<property name="subject" type="String" />
	</bean>

	<bean class="com.shopping.web.services.validation.data.CartVoucherValidationDataList">
		<property name="CartVoucherValidationDataList"
				  type="java.util.List&lt;com.shopping.web.services.validation.data.CartVoucherValidationData>"/>
	</bean>

	<bean class="com.shopping.web.services.user.data.LoginChangeResponse">
		<property name="success" type="boolean"/>
	</bean>

	<bean class="com.shopping.web.services.user.data.PasswordRestoreResponse">
		<property name="success" type="boolean"/>
	</bean>

	<!-- update queues -->

	<bean class="com.shopping.web.services.queues.data.ProductExpressUpdateElementData">
		<property name="code" type="String"/>
		<property name="catalogId" type="String"/>
		<property name="catalogVersion" type="String"/>
	</bean>

	<bean class="com.shopping.web.services.queues.data.ProductExpressUpdateElementDataList">
		<property name="productExpressUpdateElements"
		          type="java.util.List&lt;com.shopping.web.services.queues.data.ProductExpressUpdateElementData>"/>
	</bean>

	<bean class="com.shopping.web.services.queues.data.OrderStatusUpdateElementData">
		<property name="code" type="String"/>
		<property name="status" type="String"/>
		<property name="baseSiteId" type="String"/>
	</bean>

	<bean class="com.shopping.web.services.queues.data.OrderStatusUpdateElementDataList">
		<property name="orderStatusUpdateElements"
		          type="java.util.List&lt;com.shopping.web.services.queues.data.OrderStatusUpdateElementData>"/>
	</bean>

	<bean class="com.shopping.web.services.store.data.StoreCountListData">
		<property name="countriesAndRegionsStoreCount"
				  type="java.util.List&lt;de.hybris.platform.commercefacades.store.data.StoreCountData>"/>
	</bean>

	<!-- Consent management -->
	<bean class="com.shopping.web.services.consent.data.ConsentTemplateDataList">
		<property name="consentTemplates"
				  type="java.util.List&lt;de.hybris.platform.commercefacades.consent.data.ConsentTemplateData>"/>
	</bean>

	<!-- Base Sites -->
	<bean class="com.shopping.web.services.basesite.data.BaseSiteDataList">
		<property name="baseSites"
				  type="java.util.List&lt;de.hybris.platform.commercefacades.basesite.data.BaseSiteData>"/>
	</bean>

	<bean class="com.shopping.web.services.returns.data.ReturnRequestsData">
		<property name="returnRequests"
				  type="java.util.List&lt;de.hybris.platform.ordermanagementfacades.returns.data.ReturnRequestData>"/>
		<property name="sorts" type="java.util.List&lt;de.hybris.platform.commerceservices.search.pagedata.SortData>"/>
		<property name="pagination" type="de.hybris.platform.commerceservices.search.pagedata.PaginationData"/>
	</bean>
	
	<bean class="com.shopping.web.services.data.shoppingCountryConfigWsDTO">
		<description>Shopping Country Config Data Description</description>
		<property name="country" type="String">
			<description>Country ISO CODE</description>
		</property>
		<property name="currency" type="String">
			<description>Currency ISO CODE</description>
		</property>
		<property name="partRestriction" type="String">
			<description>Restricted Parts</description>
		</property>
	</bean>

</beans>
